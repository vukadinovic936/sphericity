## Download
## Rename
## Merge X chromosome
PLINK=/home/dockeruser/Documents/PLINK2/plink2

for i in 1
do

	filename="ukb${i}"
	### Check for missing
	$PLINK --pfile $filename --missing 
	Rscript --no-save hist_miss.R

	$PLINK --pfile $filename --geno 0.2 --make-pgen --out "${filename}_2"
	$PLINK --pfile "${filename}_2" --mind 0.2 --make-pgen --out "${filename}_3"
	$PLINK --pfile "${filename}_3" --geno 0.02 --make-pgen --out "${filename}_4"
	$PLINK --pfile "${filename}_4" --mind 0.02 --make-pgen --out "${filename}_5"

	## Check sex discrepancy
	$PLINK --pfile "${filename}_5" --check-sex
	Rscript --no-save gender_check.R
	grep "PROBLEM" plink.sexcheck| awk '{print$1,$2}'> sex_discrepancy.txt
	$PLINK --pfile "${filename}_5" --remove sex_discrepancy.txt --make-pgen --out "${filename}_6"

	## Select autosomal SNPs only and delete ones with low MAF distribution
	awk '{ if ($1 >= 1 && $1 <= 22) print $2 }' "${filename}_6.bim" > snp_1_22.txt
	$PLINK --pfile "${filename}_6" --extract snp_1_22.txt --make-pgen --out "${filename}_7"
	$PLINK --pfile "${filename}_7" --freq --out MAF_check
	Rscript --no-save MAF_check.R
	$PLINK --pfile "${filename}_7" --maf 0.05 --make-pgen --out "${filename}_8"

	## Hardy-Weinpgen equilibrium
	$PLINK --pfile "${filename}_8" --hardy
	awk '{ if ($9 <0.00001) print $0 }' plink.hwe>plinkzoomhwe.hwe
	Rscript --no-save hwe.R

	# there are two lines in case you want to apply different threshold for control (next line) 
	$PLINK --pfile  "${filename}_8" --hwe 1e-6 --make-pgen --out hwe_filter_step1
	$PLINK --pfile hwe_filter_step1 --hwe 1e-6 --hwe-all --make-pgen --out  "${filename}_9"

	## Heterozygosity
	$PLINK --pfile  "${filename}_9" --exclude inversion.txt --range --indep-pairwise 50 5 0.2 --out indepSNP
	$PLINK --pfile  "${filename}_9" --extract indepSNP.prune.in --het --out R_check
	Rscript --no-save check_heterozygosity_rate.R
	Rscript --no-save heterozygosity_outliers_list.R
	sed 's/"// g' fail-het-qc.txt | awk '{print$1, $2}'> het_fail_ind.txt
	$PLINK --pfile  "${filename}_9" --remove het_fail_ind.txt --make-pgen --out  "${filename}_10"
done

